function setup() {
  createCanvas(400, 400); 
  background(255); 
}

function draw() {
  let bowlColor = color(100, 150, 200, 150); 
  fill(bowlColor); 
  noStroke(); 
  ellipse(width / 2, height / 2, 200, 150); // Draws an ellipse at the center of the canvas with a width of 200 and a height of 150.
}
